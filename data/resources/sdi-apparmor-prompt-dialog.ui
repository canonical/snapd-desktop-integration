<?xml version='1.0' encoding='UTF-8'?>
<interface>
  <template class="SdiApparmorPromptDialog" parent="GtkWindow">
    <property name="title" translatable="yes">Security notification</property>
    <property name="resizable">False</property>
    <signal name="close-request" handler="close_request_cb" object="SdiApparmorPromptDialog" swapped="yes"/>
    <child>
      <object class="GtkBox">
        <property name="margin-top">20</property>
        <property name="margin-bottom">20</property>
        <property name="margin-start">20</property>
        <property name="margin-end">20</property>
        <property name="spacing">24</property>
        <child>
          <object class="GtkImage" id="image">
            <property name="valign">start</property>
            <property name="pixel-size">48</property>
          </object>
        </child>
        <child>
          <object class="GtkBox">
            <property name="orientation">vertical</property>
            <property name="spacing">16</property>
            <child>
              <object class="GtkLabel" id="header_label">
                <property name="xalign">0.0</property>
                <property name="wrap">True</property>
              </object>
            </child>
            <child>
              <object class="GtkLabel" id="more_information_link_label">
                <property name="xalign">0.0</property>
                <signal name="activate-link" handler="more_info_cb" object="SdiApparmorPromptDialog" swapped="yes"/>
              </object>
            </child>
            <child>
              <object class="GtkLabel" id="more_information_label">
                <property name="visible">False</property>
                <property name="xalign">0.0</property>
              </object>
            </child>
            <child>
              <object class="GtkBox">
                <property name="spacing">8</property>
                <property name="margin-top">8</property>
                <child>
                  <object class="GtkButton" id="allow_button">
                    <signal name="clicked" handler="allow_cb" object="SdiApparmorPromptDialog" swapped="yes"/>
                    <child>
                      <object class="GtkLabel">
                        <property name="label" translatable="yes">Allow</property>
                      </object>
                    </child>
                  </object>
                </child>
                <child>
                  <object class="GtkButton" id="deny_button">
                    <signal name="clicked" handler="deny_cb" object="SdiApparmorPromptDialog" swapped="yes"/>
                    <child>
                      <object class="GtkLabel">
                        <property name="label" translatable="yes">Deny</property>
                      </object>
                    </child>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="GtkCheckButton" id="remember_check_button">
                <property name="label" translatable="yes">Remember my decision</property>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
  </template>
</interface>
