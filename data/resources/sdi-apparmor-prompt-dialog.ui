<?xml version='1.0' encoding='UTF-8'?>
<interface>
  <template class="SdiApparmorPromptDialog" parent="GtkWindow">
    <property name="resizable">False</property>
    <property name="title" translatable="yes">Security notification</property>
    <signal name="close-request" handler="close_request_cb" object="SdiApparmorPromptDialog" swapped="yes"/>
    <child>
      <object class="GtkBox">
        <property name="margin-top">20</property>
        <property name="margin-bottom">20</property>
        <property name="margin-start">20</property>
        <property name="margin-end">20</property>
        <property name="spacing">24</property>
        <child>
          <object class="GtkImage" id="image">
            <property name="valign">start</property>
            <property name="pixel-size">48</property>
          </object>
        </child>
        <child>
          <object class="GtkBox">
            <property name="orientation">vertical</property>
            <property name="spacing">16</property>
            <child>
              <object class="GtkLabel" id="header_label">
                <property name="xalign">0.0</property>
                <property name="wrap">True</property>
              </object>
            </child>
            <child>
              <object class="GtkLabel">
                <property name="xalign">0.0</property>
                <property name="label" translatable="yes">Denying access may disrupt some application features.</property>
              </object>
            </child>
            <child>
              <object class="GtkExpander">
                <child type="label">
                  <object class="GtkLabel" id="app_details_title_label">
                    <property name="xalign">0.0</property>
                  </object>
                </child>
                <child>
                  <object class="GtkBox">
                    <property name="orientation">vertical</property>
                    <property name="spacing">4</property>
                    <property name="margin-top">12</property>
                    <style>
                      <class name="app-details"/>
                    </style>
                    <child>
                      <object class="GtkLabel" id="app_publisher_label">
                        <property name="xalign">0.0</property>
                      </object>
                    </child>
                    <child>
                      <object class="GtkLabel" id="app_updated_label">
                        <property name="xalign">0.0</property>
                      </object>
                    </child>
                    <child>
                      <object class="GtkLabel" id="app_center_label">
                        <property name="xalign">0.0</property>
                        <signal name="activate-link" handler="app_center_link_cb" object="SdiApparmorPromptDialog" swapped="yes"/>
                      </object>
                    </child>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="GtkBox" id="more_options_box">
                <property name="visible">False</property>
                <property name="orientation">vertical</property>
                <property name="spacing">4</property>
                <child>
                  <object class="GtkLabel">
                    <property name="xalign">0.0</property>
                    <property name="label" translatable="yes">Path pattern:</property>
                  </object>
                </child>
                <child>
                  <object class="GtkEntry" id="path_pattern_entry">
                  </object>
                </child>
                <child>
                  <object class="GtkLabel">
                    <property name="xalign">0.0</property>
                    <property name="label" translatable="yes">Permissions:</property>
                    <property name="margin-top">8</property>
                  </object>
                </child>
                <child>
                  <object class="GtkGrid">
                    <child>
                      <object class="GtkCheckButton" id="execute_check_button">
                        <property name="label" translatable="yes">Execute</property>
                        <layout>
                          <property name="row">0</property>
                          <property name="column">0</property>
                        </layout>
                      </object>
                    </child>
                    <child>
                      <object class="GtkCheckButton" id="write_check_button">
                        <property name="label" translatable="yes">Write</property>
                        <layout>
                          <property name="row">1</property>
                          <property name="column">0</property>
                        </layout>
                      </object>
                    </child>
                    <child>
                      <object class="GtkCheckButton" id="read_check_button">
                        <property name="label" translatable="yes">Read</property>
                        <layout>
                          <property name="row">2</property>
                          <property name="column">0</property>
                        </layout>
                      </object>
                    </child>
                    <child>
                      <object class="GtkCheckButton" id="append_check_button">
                        <property name="label" translatable="yes">Append</property>
                        <layout>
                          <property name="row">3</property>
                          <property name="column">0</property>
                        </layout>
                      </object>
                    </child>
                    <child>
                      <object class="GtkCheckButton" id="create_check_button">
                        <property name="label" translatable="yes">Create</property>
                        <layout>
                          <property name="row">4</property>
                          <property name="column">0</property>
                        </layout>
                      </object>
                    </child>
                    <child>
                      <object class="GtkCheckButton" id="delete_check_button">
                        <property name="label" translatable="yes">Delete</property>
                        <layout>
                          <property name="row">5</property>
                          <property name="column">0</property>
                        </layout>
                      </object>
                    </child>
                    <child>
                      <object class="GtkCheckButton" id="open_check_button">
                        <property name="label" translatable="yes">Open</property>
                        <layout>
                          <property name="row">0</property>
                          <property name="column">1</property>
                        </layout>
                      </object>
                    </child>
                    <child>
                      <object class="GtkCheckButton" id="rename_check_button">
                        <property name="label" translatable="yes">Rename</property>
                        <layout>
                          <property name="row">1</property>
                          <property name="column">1</property>
                        </layout>
                      </object>
                    </child>
                    <child>
                      <object class="GtkCheckButton" id="set_attribute_check_button">
                        <property name="label" translatable="yes">Set attribute</property>
                        <layout>
                          <property name="row">2</property>
                          <property name="column">1</property>
                        </layout>
                      </object>
                    </child>
                    <child>
                      <object class="GtkCheckButton" id="get_attribute_check_button">
                        <property name="label" translatable="yes">Get attribute</property>
                        <layout>
                          <property name="row">3</property>
                          <property name="column">1</property>
                        </layout>
                      </object>
                    </child>
                    <child>
                      <object class="GtkCheckButton" id="change_mode_check_button">
                        <property name="label" translatable="yes">Change mode</property>
                        <layout>
                          <property name="row">4</property>
                          <property name="column">1</property>
                        </layout>
                      </object>
                    </child>
                    <child>
                      <object class="GtkCheckButton" id="change_owner_check_button">
                        <property name="label" translatable="yes">Change owner</property>
                        <layout>
                          <property name="row">5</property>
                          <property name="column">1</property>
                        </layout>
                      </object>
                    </child>
                    <child>
                      <object class="GtkCheckButton" id="change_group_check_button">
                        <property name="label" translatable="yes">Change group</property>
                        <layout>
                          <property name="row">0</property>
                          <property name="column">2</property>
                        </layout>
                      </object>
                    </child>
                    <child>
                      <object class="GtkCheckButton" id="lock_check_button">
                        <property name="label" translatable="yes">Lock</property>
                        <layout>
                          <property name="row">1</property>
                          <property name="column">2</property>
                        </layout>
                      </object>
                    </child>
                    <child>
                      <object class="GtkCheckButton" id="execute_map_check_button">
                        <property name="label" translatable="yes">Execute map</property>
                        <layout>
                          <property name="row">2</property>
                          <property name="column">2</property>
                        </layout>
                      </object>
                    </child>
                    <child>
                      <object class="GtkCheckButton" id="link_check_button">
                        <property name="label" translatable="yes">Link</property>
                        <layout>
                          <property name="row">3</property>
                          <property name="column">2</property>
                        </layout>
                      </object>
                    </child>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="GtkBox">
                <property name="spacing">8</property>
                <property name="margin-top">8</property>
                <child>
                  <object class="GtkButton">
                    <signal name="clicked" handler="always_allow_cb" object="SdiApparmorPromptDialog" swapped="yes"/>
                    <child>
                      <object class="GtkLabel">
                        <property name="label" translatable="yes">Always allow</property>
                      </object>
                    </child>
                  </object>
                </child>
                <child>
                  <object class="GtkButton" id="allow_once_button">
                    <property name="visible">False</property>
                    <signal name="clicked" handler="allow_once_cb" object="SdiApparmorPromptDialog" swapped="yes"/>
                    <child>
                      <object class="GtkLabel">
                        <property name="label" translatable="yes">Allow once</property>
                      </object>
                    </child>
                  </object>
                </child>
                <child>
                  <object class="GtkButton" id="always_deny_button">
                    <property name="visible">False</property>
                    <signal name="clicked" handler="always_deny_cb" object="SdiApparmorPromptDialog" swapped="yes"/>
                    <child>
                      <object class="GtkLabel">
                        <property name="label" translatable="yes">Always deny</property>
                      </object>
                    </child>
                  </object>
                </child>
                <child>
                  <object class="GtkButton">
                    <signal name="clicked" handler="deny_once_cb" object="SdiApparmorPromptDialog" swapped="yes"/>
                    <child>
                      <object class="GtkLabel">
                        <property name="label" translatable="yes">Deny once</property>
                      </object>
                    </child>
                  </object>
                </child>
                <child>
                  <object class="GtkButton" id="more_options_button">
                    <signal name="clicked" handler="more_options_cb" object="SdiApparmorPromptDialog" swapped="yes"/>
                    <child>
                      <object class="GtkLabel">
                        <property name="label" translatable="yes">More options…</property>
                      </object>
                    </child>
                  </object>
                </child>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
  </template>
</interface>
